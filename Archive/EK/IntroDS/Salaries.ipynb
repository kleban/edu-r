   "source": [
    "library(dplyr)\n",
    "library(tidyr)\n",
    "data %>% filter(job_title %in% c(\"Data Scientist\", \"Data Engineer\")) |>\n",
    "    group_by(experience_level, job_title, work_year) |>\n",
    "    summarise(avg = mean(salary)) |>\n",
    "    spread(work_year, avg)"
   ]
